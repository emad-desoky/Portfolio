{
    "extends": ["next/babel", "next/core-web-vitals"],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false,
      "babelOptions": {
        "presets": ["next/babel"]
      }
    },
    "plugins": ["react"],
    "env": {
      "browser": true,
      "es2021": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error"
    }
  }
  